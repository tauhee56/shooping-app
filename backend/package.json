{
  "name": "backend",
  "version": "1.0.0",
  "main": "dist/src/server.js",
  "scripts": {
    "build": "node -e \"require('fs').rmSync('dist',{recursive:true,force:true})\" && tsc -p tsconfig.json",
    "start": "node dist/src/server.js",
    "smoke:test": "npm run build && node dist/scripts/smoke-test.js",
    "cleanup:demo": "npm run build && node dist/scripts/cleanup-demo.js",
    "dev": "nodemon --watch src --watch models --watch routes --ext ts --exec \"npm run build && node dist/src/server.js\"",
    "test": "jest --config jest.config.js --runInBand",
    "test:watch": "jest --config jest.config.js --watch"
  },
