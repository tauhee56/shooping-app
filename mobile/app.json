          "image": "./assets/images/splash-icon.png",
          "imageWidth": 200,
          "resizeMode": "contain",
          "backgroundColor": "#ffffff"
        }
      ],
      [
        "@stripe/stripe-react-native",
        {
          "merchantIdentifier": "merchant.com.kalshopping.app"
        }
      ],
      "@react-native-firebase/app",
      "@react-native-google-signin/google-signin",
      "expo-router"
    ],
    "updates": {
      "enabled": false
